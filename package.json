{
  "name": "nodejs-interview",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "node": "TS_NODE_TRANSPILE_ONLY=true NODE_OPTIONS=${npm_package_config_node_options} node",
    "mocha": "TZ=UTC NODE_OPTIONS=${npm_package_config_node_options} TS_NODE_TRANSPILE_ONLY=true mocha --timeout 10000 --colors --exit './{src,migrations}/**/*.test.ts'",
    "build": "webpack",
    "start": "nodemon --ext ts --watch src --watch types --exec 'npm run start:local'",
    "start:local": "npm run node -- src/server.ts",
    "start:services": "docker-compose rm --force && docker-compose up --build",
    "test": "npm run mocha -- 'src/**/*.spec.ts' --exclude '**/flycheck_*.ts'",
    "test:types": "tsc --noEmit --project .",
    "lint": "eslint '**/*.ts'",
    "prettier": "prettier '**/*.ts' -w"
  },
  "config": {
    "node_options": "--require ts-node/register --require tsconfig-paths/register --require reflect-metadata --require dotenv/config"
  },
  "dependencies": {
    "@types/chai-subset": "^1.3.3",
    "@types/sinon-chai": "^3.2.5",
    "chai-subset": "^1.6.0",
    "cors": "^2.8.5",
    "date-fns": "^2.25.0",
    "express": "^4.17.1",
    "knex": "^0.95.11",
    "pg": "^8.7.1",
    "ramda": "^0.27.1",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "serverless-http": "^2.4.1",
    "sinon-chai": "^3.7.0",
    "useragent": "^2.3.0",
    "uuid": "^8.0.0",
    "winston": "^3.3.3",
    "zod": "^3.11.4"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/chai-http": "^4.2.0",
    "@types/ramda": "^0.27.45",
    "@types/chai-as-promised": "^7.1.4",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.9",
    "@types/mocha": "^7.0.2",
    "@types/node": "12.12.31",
    "@types/node-fetch": "^2.5.11",
    "@types/sinon": "^10.0.0",
    "@types/supertest": "^2.0.11",
    "@types/useragent": "^2.1.1",
    "@types/uuid": "^7.0.3",
    "@types/webpack": "^4.41.18",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "chai-as-promised": "^7.1.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "glob": "7.1.6",
    "lint-staged": "^10.2.7",
    "mocha": "^7.1.2",
    "mocha-dd-reporter": "^1.1.1",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "raw-loader": "^4.0.2",
    "sinon": "^9.2.4",
    "supertest": "^6.1.3",
    "ts-loader": "^7.0.3",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.3.2",
    "validator": "^13.1.1",
    "webpack": "4.42.1",
    "webpack-cli": "3.3.11"
  }
}
